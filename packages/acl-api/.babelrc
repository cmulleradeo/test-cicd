{
  "env": {
    "test": {
      "plugins": [
        "@babel/plugin-transform-modules-commonjs",
        "@babel/plugin-proposal-optional-chaining",
        "@babel/plugin-proposal-class-properties"
      ],
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": [
              "node 10"
            ]
          }
        ]
      ]
    },
    "production": {
      "plugins": [
        "@babel/plugin-transform-modules-commonjs",
        "@babel/plugin-proposal-optional-chaining",
        "@babel/plugin-proposal-class-properties"
      ],
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": [
              "node 10"
            ],
            "modules": false
          }
        ]
      ]
    },
    "development": {
      "plugins": [
        "@babel/plugin-transform-modules-commonjs",
        "@babel/plugin-proposal-optional-chaining",
        "@babel/plugin-proposal-class-properties"
      ],
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": [
              "node 10"
            ],
            "modules": false
          }
        ]
      ]
    }
  }
}
